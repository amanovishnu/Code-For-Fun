<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <TextField fx:id="address" layoutX="41.0" layoutY="74.0" prefWidth="220.0" promptText="Email Address" />
      <ComboBox fx:id="combobox" layoutX="41.0" layoutY="116.0" prefWidth="220.0" promptText="What is Your Security Question ?" />
      <Label alignment="CENTER" layoutX="70.0" layoutY="21.0" prefHeight="36.0" prefWidth="161.0" text="Password Recovery">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="40.0" layoutY="208.0" mnemonicParsing="false" onAction="#recover" prefWidth="220.0" text="Recover" />
      <TextField fx:id="answer" layoutX="41.0" layoutY="164.0" prefWidth="220.0" promptText="Security Answer" />
      <Label fx:id="reloginstatus" alignment="CENTER" layoutX="90.0" layoutY="249.0" prefHeight="36.0" prefWidth="132.0" textFill="#314cd3" />
   </children>
</AnchorPane>
